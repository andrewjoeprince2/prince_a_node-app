<a href="/">
  <i class="fa-fa-home"></i>
</a>

<div class="page-header">
<h3>{{ title }}</h3>
</div>


  <div class="jumbotron">
    <video src="../videos/darknight.mp4" autoplay controls poster="../images/darknight.jpg"></video>
  </div>

  <div class="panel panel-default">
    <div class="panel-heading" id="vidInfo">
      <h3 class="panel-title">Content Loading</h3>
    </div>
    <div class="panel-body">
      <p id="vidDesc">Content Loading</p>

      <ul id="movieSocial">
        <li><a id="vidFb" href="#"><i class="fa fa-facebook-official" aria-hidden="true"></i></a></li>
        <li><a id="vidTwitter" href="#"><i class="fa fa-twitter" aria-hidden="true"></i></a></li>
        <li><a id="vidInstagram" href="#"><i class="fa fa-instagram" aria-hidden="true"></i></a></li>
        <li><a id="vidUrl" href="#"><i class="fa fa-link" aria-hidden="true"></i></a></li>
      </ul>

    </div>
  </div>



<div class="row" id="thumbs">

<ul>
  {{#each videoData}}
  <li>
  <div class="col-xs-6 col-sm-4 col-lg-2">
    <div class="thumbnail">
      <img src="../images/{{ videos_thumb }}" alt="{{ videos_title }} Thumbnail" id="{{ videos_id }}">
      <div class="caption">
        <h3>{{ videos_title }}</h3>
      </div>
    </div>
  </div>
</li>
  {{/each}}
</ul>

</div>
